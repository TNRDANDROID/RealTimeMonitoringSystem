<?xml version="1.0" encoding="utf-8"?>
<layout>
    <data>
        <variable
            name="adapter"
            type="com.nic.RealTimeMonitoringSystem.adapter.WorkListAdapter" />
    </data>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/work_layout"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/workid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="5dip"
        android:text="workid"
         android:textSize="16sp"
        android:textColor="#FF4500"
        android:textStyle="bold" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="7"
        android:gravity="center_vertical" >

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="Scheme Group Name"
            android:layout_weight="2.5"
            android:textSize="15sp"
            android:textStyle="bold"
            android:textColor="#000000" />

        <TextView
            android:id="@+id/tv_schemeGroupName"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:text="TextView"
            android:layout_weight="4.5"
            android:textSize="15sp"
            android:textColor="#000000" />
    </LinearLayout>


   <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="10"
        android:gravity="center_vertical" >

        <TextView
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="Scheme Name"
            android:layout_weight="3.5"
            android:textSize="15sp"
            android:textStyle="bold"
            android:textColor="#000000" />

        <TextView
            android:id="@+id/tv_scheme"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"

            android:text="TextView"
            android:layout_weight="6.5"
           android:textSize="15sp"
            android:textColor="#000000" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="10"
        android:gravity="center_vertical" >

        <TextView
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="Financial Year"
            android:layout_weight="3.5"
            android:textSize="15sp"
            android:textStyle="bold"
            android:textColor="#000000" />

        <TextView
            android:id="@+id/tv_financialYear"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"

            android:text="TextView"
            android:layout_weight="6.5"
           android:textSize="15sp"
            android:textColor="#000000" />
    </LinearLayout>

     <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="10"
        android:gravity="center_vertical" >

        <TextView
            android:id="@+id/textView5"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="Agency Name"
            android:layout_weight="3.5"
            android:textSize="15sp"
            android:textStyle="bold"
            android:textColor="#000000" />

        <TextView
            android:id="@+id/tv_agencyName"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="6.5"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"

            android:text="TextView"
            android:textSize="15sp"
            android:textColor="#000000" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="10"
        android:gravity="center_vertical" >

        <TextView
            android:id="@+id/textView6"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="Work Group Name"
            android:layout_weight="3.5"
            android:textSize="15sp"
            android:textStyle="bold"
            android:textColor="#000000" />

        <TextView
            android:id="@+id/tv_workGroupName"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"

            android:layout_weight="6.5"
            android:text="TextView"
           android:textSize="15sp"
            android:textColor="#000000" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="10"
        android:gravity="center_vertical" >

        <TextView
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="Work Name"
            android:layout_weight="3.5"
            android:textSize="15sp"
            android:textStyle="bold"
            android:textColor="#000000" />

        <TextView
            android:id="@+id/tv_workName"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"

            android:layout_weight="6.5"
            android:text="TextView"
            android:textSize="15sp"
            android:textColor="#000000" />
    </LinearLayout>

     <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="10"
        android:gravity="center_vertical" >

        <TextView
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="Block"
            android:layout_weight="3.5"
           android:textSize="15sp"
           android:textStyle="bold"
            android:textColor="#000000" />

        <TextView
            android:id="@+id/tv_block"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"

            android:text="TextView"
            android:layout_weight="6.5"
            android:textSize="15sp"
            android:textColor="#000000" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="10"
        android:gravity="center_vertical" >

        <TextView
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_weight="3.5"
            android:text="Village"
            android:textSize="15sp"
            android:textStyle="bold"
            android:textColor="#000000" />

        <TextView
            android:id="@+id/tv_village"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_marginLeft="6dp"
            android:layout_weight="6.5"
            android:text="TextView"
           android:textSize="15sp"
            android:textColor="#000000" />
    </LinearLayout>

     <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="10"
        android:gravity="center_vertical" >

        <TextView
            android:id="@+id/textView10"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="Current Stage"
            android:textStyle="bold"
            android:layout_weight="3.5"
            android:textSize="15sp"
            android:textColor="#000000" />

        <TextView
            android:id="@+id/tv_currentStage"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_marginLeft="6dp"

            android:text="TextView"
            android:layout_weight="6.5"
           android:textSize="15sp"
            android:textColor="#000000" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="10"
        android:gravity="center_vertical" >

        <TextView
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="3.5"
            android:layout_marginLeft="10dp"
            android:textStyle="bold"
            android:text="Beneficiary \nName"
           android:textSize="15sp"
            android:textColor="#000000" />

        <TextView
            android:id="@+id/tv_beneficiaryFatherName"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:text="BeneficiaryFatherName"
            android:layout_marginLeft="6dp"

            android:layout_weight="6.5"
            android:textSize="15sp"
            android:textColor="#000000" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="10"
        android:gravity="center_vertical" >

        <TextView
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:textStyle="bold"
            android:text="Gender"
            android:layout_weight="3.5"
            android:textSize="15sp"
            android:textColor="#000000" />

        <TextView
            android:id="@+id/tv_gender"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:text="MAle"
            android:layout_marginLeft="6dp"

            android:layout_weight="6.5"
            android:textSize="15sp"
            android:textColor="#000000" />
    </LinearLayout>

     <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="10"
        android:gravity="center_vertical" >

        <TextView
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dip"
            android:text="Community"
            android:layout_weight="3.5"
            android:textStyle="bold"
            android:textSize="15sp"
            android:textColor="#000000" />

        <TextView
            android:id="@+id/tv_community"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:text="OBC"
            android:layout_marginLeft="6dp"

            android:layout_weight="6.5"
            android:textSize="15sp"
            android:textColor="#000000" />
    </LinearLayout>

 <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="10"
        android:gravity="center_vertical" >
        <TextView
            android:id="@+id/initialAmount"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dip"
            android:layout_weight="3.5"
            android:textStyle="bold"
            android:text="Initial Amount"
            android:textSize="15sp"
            android:textColor="#000000" />

        <TextView
            android:id="@+id/tv_initialAmount"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:text="10000"
            android:layout_marginLeft="6dp"

            android:layout_weight="6.5"
           android:textSize="15sp"
            android:textColor="#000000" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="10"
        android:gravity="center_vertical" >

        <TextView
            android:id="@+id/amountSpentSoFar"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_weight="3.5"
            android:textStyle="bold"
            android:text="Amount Spent So Far"
            android:textSize="15sp"
            android:textColor="#000000" />

        <TextView
            android:id="@+id/tv_amountSpentSoFar"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="6.5"
            android:text="10000"
            android:layout_marginLeft="6dp"

            android:textSize="15sp"
            android:textColor="#000000" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="10"
        android:gravity="center_vertical" >
        
        <TextView
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_weight="3.5"
            android:text="Last Visited Date"
            android:textStyle="bold"
            android:textSize="15sp" 
            android:textColor="#000000"/>

        <TextView
            android:id="@+id/tv_last_visited_date"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="6.5"
            android:text="Date"
            android:layout_marginLeft="6dp"

            android:textSize="15sp"
            android:textColor="#000000" />
    </LinearLayout>

</LinearLayout>
</layout>